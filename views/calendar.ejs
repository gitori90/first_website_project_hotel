
<div class="container-fluid calendar-wrapper">
<form class="" action="" method="post">


<div class="calender-top">
  <ul class="calendar-top-list">

  <li class="top-item"><i class="fas fa-chevron-left top-left-arrow"></i></li>

  <li class="month-label top-item"></li>

  <li class="top-item"><i class="fas fa-chevron-right top-right-arrow"></i></li>

  </ul>
</div>
<table class="calendar">
  <!-- <caption class="month-label"></caption> -->
  <tr class="header">
    <th class="calendarHeader">Sun</th>
    <th class="calendarHeader">Mon</th>
    <th class="calendarHeader">Tue</th>
    <th class="calendarHeader">Wed</th>
    <th class="calendarHeader">Thu</th>
    <th class="calendarHeader">Fri</th>
    <th class="calendarHeader">Sat</th>
  </tr>

</table>
<button type="submit" class="calendar-submit-btn">Confirm</button>
</form>
</div>

<script type="text/javascript">
<%- include("calendar.js") -%>
</script>
